{
  "name": "JS-Logger",
  "version": "1.0.0",
  "description": "Logging all the front end information in logger which will help you identify error or bugs on production site",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "start-watch": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --runInBand",
    "test:coverage": "jest  --runInBand --coverage",
    "migrate-mongo": "cd server && migrate-mongo",
    "migrate_down": "cd server && migrate-mongo down",
    "migrate_up": "cd server && migrate-mongo up",
    "migrate_status": "cd server && migrate-mongo status",
    "lint": "eslint ./server",
    "lint:fix": "eslint ./server --fix",
    "prettier:fix": "prettier ./server --write"
  },

  "lint-staged": {
    "**/server/*.{js,json}": [
      "npm run prettier:fix",
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supreetssethi/JS-Logger.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supreetssethi/JS-Logger/issues"
  },
  "homepage": "https://github.com/supreetssethi/JS-Logger#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-subdomain": "^1.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "migrate-mongo": "^7.2.1",
    "mongoose": "^5.9.21",
    "request": "^2.88.2",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",

    "jest": "^26.1.0",
    "jsdoc": "^3.6.4",
    "lint-staged": "^10.2.11",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}